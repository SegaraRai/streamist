{
  "compilerOptions": {
    "isolatedModules": true,
    "esModuleInterop": true,
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "baseUrl": ".",
    "paths": {
      "$shared/*": [
        "../shared/src/*"
      ],
      "$prisma/*": [
        "../server/node_modules/.prisma/*"
      ],
      ".prisma/*": [
        "../server/node_modules/.prisma/*"
      ]
    },
    "strict": true
  },
}
