{
  "compilerOptions": {
    "isolatedModules": true,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "lib": [
      "dom"
    ],
    "module": "ESNext",
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {
      "$prisma/*": [
        "./node_modules/.prisma/*"
      ],
      ".prisma/*": [
        "./node_modules/.prisma/*"
      ],
      "$shared/*": [
        "../shared/*"
      ],
      "$shared-server/*": [
        "../shared-server/*"
      ],
      "$transcoder/*": [
        "../transcoder/src/*"
      ],
      "$/*": [
        "./*"
      ],
      "class-transformer": [
        "./node_modules/class-transformer/esm2015"
      ],
    },
    "rootDirs": [
      ".",
      "../shared",
      "../shared-server",
      "../transcoder",
    ],
    "skipLibCheck": true,
    "strict": true,
    "target": "ESNext"
  }
}
