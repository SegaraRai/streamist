{
  "name": "frourio-app-server",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run migrate:dev && run-p dev:*",
    "dev:server": "run-p dev_server:*",
    "dev:frourio": "frourio --watch",
    "dev:prisma": "prisma generate --watch",
    "dev_server:build": "node ./build.dev.js",
    "dev_server:start": "node-dev --respawn ./index.js",
    "prebuild": "npm run generate",
    "build": "webpack --mode=production",
    "generate": "run-p generate:*",
    "generate:frourio": "frourio",
    "generate:prisma": "prisma generate",
    "migrate:dev": "prisma migrate dev",
    "migrate:dev:createonly": "prisma migrate dev --create-only",
    "migrate:reset": "prisma migrate reset",
    "pm2:start": "pm2 start pm2.config.json --env production",
    "pm2:stop": "pm2 stop pm2.config.json",
    "pm2:delete": "pm2 delete pm2.config.json",
    "pm2:logs": "pm2 logs",
    "pm2:monit": "pm2 monit",
    "start": "cross-env NODE_ENV=production node index.js"
  },
  "dependencies": {
    "@aws-sdk/client-lambda": "^3.43.0",
    "@aws-sdk/client-s3": "^3.44.0",
    "@aws-sdk/s3-request-presigner": "^3.44.0",
    "@prisma/client": "^3.5.0",
    "class-validator": "^0.13.2",
    "dotenv": "^10.0.0",
    "fast-json-stringify": "^2.7.12",
    "fastify": "^3.24.0",
    "fastify-cors": "^6.0.2",
    "fastify-helmet": "^5.3.2",
    "fastify-jwt": "^4.0.0",
    "fastify-multipart": "^5.1.0",
    "fastify-static": "^4.5.0",
    "frourio": "^0.25.1",
    "node-fetch": "^3.1.0",
    "pm2": "^5.1.2"
  },
  "devDependencies": {
    "@types/busboy": "^0.3.1",
    "@types/node": "^16.11.11",
    "cross-env": "^7.0.3",
    "esbuild": "^0.14.1",
    "esbuild-node-externals": "^1.4.0",
    "nodemon-webpack-plugin": "^4.6.0",
    "npm-run-all": "^4.1.5",
    "prisma": "^3.5.0",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.5.2",
    "webpack": "^5.64.4",
    "webpack-cli": "^4.9.1",
    "webpack-node-externals": "^3.0.0"
  }
}
