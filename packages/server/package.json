{
  "name": "frourio-app-server",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "pnpm run migrate:dev && run-p dev:*",
    "dev:server": "run-p dev_server:*",
    "dev:frourio": "frourio --watch",
    "dev_server:build": "node ./build.dev.js",
    "dev_server:start": "node-dev --respawn ./index.js",
    "prebuild": "pnpm run generate",
    "build": "webpack --mode=production",
    "generate": "run-p generate:*",
    "generate:frourio": "frourio",
    "generate:prisma": "prisma generate && node deleteBadPrismaSchema.js",
    "migrate:dev": "prisma migrate dev && node deleteBadPrismaSchema.js",
    "migrate:dev:createonly": "prisma migrate dev --create-only && node deleteBadPrismaSchema.js",
    "migrate:reset": "prisma migrate reset",
    "pm2:start": "pm2 start pm2.config.json --env production",
    "pm2:stop": "pm2 stop pm2.config.json",
    "pm2:delete": "pm2 delete pm2.config.json",
    "pm2:logs": "pm2 logs",
    "pm2:monit": "pm2 monit",
    "start": "cross-env NODE_ENV=production node index.js",
    "typecheck": "tsc --noEmit",
    "pretest": "pnpm run generate",
    "test": "vitest"
  },
  "dependencies": {
    "@aws-sdk/client-lambda": "^3.45.0",
    "@aws-sdk/client-s3": "^3.45.0",
    "@aws-sdk/s3-request-presigner": "^3.45.0",
    "@prisma/client": "^3.7.0",
    "aspida": "^1.7.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dotenv": "^10.0.0",
    "fast-json-stringify": "^3.0.0",
    "fast-jwt": "^1.4.1",
    "fastify": "^3.25.3",
    "fastify-cookie": "^5.5.0",
    "fastify-cors": "^6.0.2",
    "fastify-helmet": "^5.3.2",
    "fastify-jwt": "^4.1.3",
    "fastify-multipart": "^5.2.1",
    "fastify-static": "^4.5.0",
    "node-fetch": "^3.1.0",
    "pm2": "^5.1.2",
    "reflect-metadata": "^0.1.13",
    "tsd": "^0.19.1",
    "velona": "^0.7.0"
  },
  "devDependencies": {
    "@segararai/frourio": "^0.25.1-fork",
    "@types/busboy": "^0.3.1",
    "@types/node": "^17.0.6",
    "cross-env": "^7.0.3",
    "esbuild": "^0.14.10",
    "esbuild-node-externals": "^1.4.1",
    "node-dev": "^7.1.0",
    "npm-run-all": "^4.1.5",
    "prisma": "^3.7.0",
    "ts-loader": "^9.2.6",
    "typescript": "npm:@typescript-deploys/pr-build@4.6.0-pr-47183-2",
    "vite": "^2.7.10",
    "vitest": "^0.0.134"
  }
}
