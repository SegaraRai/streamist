{
  "name": "@streamist/frontend",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "preview": "vite preview",
    "preview-https": "serve dist",
    "dev": "run-p dev:*",
    "dev:client": "vite --port 3333 --open",
    "dev:aspida": "aspida --watch",
    "prebuild": "pnpm run generate",
    "build": "vite build",
    "generate": "aspida",
    "lint": "pnpm run lint:js && pnpm run lint:style",
    "lint:js": "eslint --ext .ts,.js,.vue . && prettier --check \"./**/*.ts\"",
    "lint:style": "stylelint **/*.{vue,css} --ignore-path .gitignore",
    "lint:fix": "eslint --ext .ts,.js,.vue . --fix && prettier --write \"./**/*.ts\" && pnpm run lint:style -- --fix",
    "start": "nuxt serve",
    "test": "run-s test:*",
    "test:unit": "vitest",
    "test_ci": "run-s test_ci:*",
    "test_ci:unit": "vitest --run",
    "test_dev": "run-s test:*",
    "test_dev:unit": "vitest dev",
    "pretest": "pnpm run generate",
    "pretypecheck": "pnpm run generate",
    "typecheck": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@aspida/axios": "^1.9.1",
    "@mdi/font": "^6.6.96",
    "@prisma/client": "^3.12.0",
    "@vueuse/core": "^8.2.6",
    "@vueuse/head": "^0.7.6",
    "axios": "^0.26.1",
    "bcp-47-match": "^2.0.2",
    "buffer": "^6.0.3",
    "class-validator": "^0.13.2",
    "core-js": "^3.22.0",
    "dexie": "^3.2.1",
    "fuse.js": "^6.5.3",
    "humanize-duration": "^3.27.1",
    "lazysizes": "^5.3.2",
    "naive-ui": "^2.28.0",
    "nprogress": "^0.2.0",
    "p-queue": "^7.2.0",
    "pinia": "^2.0.13",
    "prisma": "^3.12.0",
    "tsd": "^0.20.0",
    "ua-parser-js": "^1.0.2",
    "vue": "^3.2.33",
    "vue-demi": "^0.12.5",
    "vue-i18n": "^9.2.0-beta.23",
    "vue-router": "^4.0.14",
    "vuedraggable": "^4.1.0",
    "vuetify": "^3.0.0-beta.0",
    "workbox-window": "^6.5.3"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.5.1",
    "@iconify-json/mdi": "^1.1.9",
    "@intlify/vite-plugin-vue-i18n": "^3.4.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@types/humanize-duration": "^3.27.1",
    "@types/nprogress": "^0.2.0",
    "@types/ua-parser-js": "^0.7.36",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vuetify/vite-plugin": "^1.0.0-alpha.10",
    "aspida": "^1.9.1",
    "critters": "^0.0.16",
    "dotenv": "^16.0.0",
    "fast-glob": "^3.2.11",
    "rollup": "^2.70.2",
    "unplugin-auto-import": "^0.7.1",
    "unplugin-icons": "^0.14.1",
    "unplugin-vue-components": "^0.19.3",
    "vite": "^2.9.5",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-inspect": "^0.5.0",
    "vite-plugin-pages": "^0.22.0",
    "vite-plugin-pwa": "^0.11.13",
    "vite-plugin-vue-layouts": "^0.6.0",
    "vite-plugin-windicss": "^1.8.4",
    "vite-ssg": "^0.19.2",
    "vitest": "^0.9.3",
    "vue-tsc": "^0.34.7",
    "windicss": "^3.5.1"
  }
}
