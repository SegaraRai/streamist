{
  "private": true,
  "scripts": {
    "preview": "vite preview",
    "preview-https": "serve dist",
    "dev": "run-p dev:*",
    "dev:client": "vite --port 3333 --open",
    "dev:aspida": "aspida --watch",
    "prebuild": "npm run generate",
    "build": "cross-env NODE_ENV=production vite-ssg build",
    "generate": "aspida",
    "lint": "npm run lint:js && npm run lint:style",
    "lint:js": "eslint --ext .ts,.js,.vue . && prettier --check \"./**/*.ts\"",
    "lint:style": "stylelint **/*.{vue,css} --ignore-path .gitignore",
    "lint:fix": "eslint --ext .ts,.js,.vue . --fix && prettier --write \"./**/*.ts\" && npm run lint:style -- --fix",
    "start": "nuxt serve",
    "pretest": "npm run generate",
    "test": "jest",
    "pretypecheck": "npm run generate",
    "typecheck": "vue-tsc --noEmit"
  },
  "dependencies": {
